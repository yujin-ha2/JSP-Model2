<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>GoodFunding</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>MemberFrontController</servlet-name>
    <servlet-class>net.member.action.MemberFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MemberFrontController</servlet-name>
    <url-pattern>*.me</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FundingOpenController</servlet-name>
    <servlet-class>net.funding.open.action.FundingOpenController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FundingOpenController</servlet-name>
    <url-pattern>*.on</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FundingController</servlet-name>
    <servlet-class>net.funding.action.FundingController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FundingController</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminController</servlet-name>
    <servlet-class>net.admin.action.AdminController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminController</servlet-name>
    <url-pattern>*.ad</url-pattern>
  </servlet-mapping>
  <resource-ref>
    <description>Connection</description>
    <res-ref-name>jdbc/funding</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
</web-app>